<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Colo Shop Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@
    @Styles.Render("~/Content/styles/bootstrap4/bootstrap.min.css")
    @Styles.Render("~/Content/plugins/font-awesome-4.7.0/css/font-awesome.min.css")
    @Styles.Render("~/Content/plugins/OwlCarousel2-2.2.1/owl.carousel.css")
    @Styles.Render("~/Content/plugins/OwlCarousel2-2.2.1/owl.theme.default.css")
    @Styles.Render("~/Content/plugins/OwlCarousel2-2.2.1/animate.css")

    @Styles.Render("~/Content/styles/main_styles.css")

    @Styles.Render("~/Content/styles/responsive.css")

    @Scripts.Render("~/Scripts/theme/jquery-3.2.1.min.js")
</head>

<body>

    <div class="super_container">

        <!-- Header -->

        <header class="header trans_300">

            <!-- Top Navigation -->

            <div class="top_nav">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="top_nav_left"><a href="/"><img src="~/images/logo.png" /></a> &nbsp; free shipping on all u.s orders over $50</div>
                        </div>
                        <div class="col-md-6 text-right">
                            <div class="top_nav_right">
                                <ul class="top_nav_menu">

                                    <!-- Currency / Language / My Account -->

                                    <li class="currency">
                                        <a href="#">
                                            Humidors
                                            <i class="fa fa-angle-down"></i>
                                        </a>
                                        <ul class="currency_selection">
                                            <li><a href="/search?categoryID=1&humidorSizeID=1">Large</a></li>
                                            <li><a href="/search?categoryID=1&humidorSizeID=2">Medium</a></li>
                                            <li><a href="/search?categoryID=1&humidorSizeID=3">Small</a></li>
                                            <li><a href="/search?categoryID=1&humidorSizeID=4">Travel</a></li>
                                        </ul>
                                    </li>
                                    <li class="language">
                                        <a href="#">
                                            Accessories
                                            <i class="fa fa-angle-down"></i>
                                        </a>
                                        <ul class="language_selection">
                                            <li><a href="/search?categoryID=2">Hygrometers</a></li>
                                            <li><a href="/search?categoryID=3">Liquids</a></li>
                                            <li><a href="/search?categoryID=4">Lighters</a></li>
                                            <li><a href="/search?categoryID=5">Cutters</a></li>
                                            <li><a href="/search?categoryID=6">Ash Trays</a></li>
                                        </ul>
                                    </li>
                                    <li class="account">
                                        <a href="#">
                                            My Account
                                            <i class="fa fa-angle-down"></i>
                                        </a>
                                        <ul class="account_selection">
                                            <li><a href="#"><i class="fa fa-sign-in" aria-hidden="true"></i>Sign In</a></li>
                                            <li><a href="#"><i class="fa fa-user-plus" aria-hidden="true"></i>Register</a></li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Main Navigation -->

            <div class="main_nav_container">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 text-right">
                            <div class="logo_container">
                                <a href="/">Famous<span>Humidors</span></a>
                            </div>
                            <nav class="navbar">
                                <ul class="navbar_menu">
                                    <li><a href="/">home</a></li>
                                    <li><a href="/search">shop</a></li>
                                    <li><a href="/about">about</a></li>
                                    <li><a href="/contact">contact</a></li>
                                </ul>
                                <ul class="navbar_user">
                                    <li><a href="/search"><i class="fa fa-search" aria-hidden="true"></i></a></li>
                                    <li><a href="/account"><i class="fa fa-user" aria-hidden="true"></i></a></li>
                                    <li class="checkout">
                                        <a href="/cart">
                                            <i class="fa fa-shopping-cart" aria-hidden="true"></i>
                                            <span id="checkout_items" class="checkout_items">2</span>
                                        </a>
                                    </li>
                                </ul>
                                <div class="hamburger_container">
                                    <i class="fa fa-bars" aria-hidden="true"></i>
                                </div>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>

        </header>

        <div class="fs_menu_overlay"></div>
        <div class="hamburger_menu">
            <div class="hamburger_close"><i class="fa fa-times" aria-hidden="true"></i></div>
            <div class="hamburger_menu_content text-right">
                <ul class="menu_top_nav">
                    <li class="menu_item has-children">
                        <a href="#">
                            Humidors
                            <i class="fa fa-angle-down"></i>
                        </a>
                        <ul class="menu_selection">
                            <li><a href="/search?categoryID=1&humidorSizeID=1">Large</a></li>
                            <li><a href="/search?categoryID=1&humidorSizeID=2">Medium</a></li>
                            <li><a href="/search?categoryID=1&humidorSizeID=3">Small</a></li>
                            <li><a href="/search?categoryID=1&humidorSizeID=4">Travel</a></li>
                        </ul>
                    </li>
                    <li class="menu_item has-children">
                        <a href="#">
                            Accessories
                            <i class="fa fa-angle-down"></i>
                        </a>
                        <ul class="menu_selection">
                            <li><a href="/search?categoryID=2">Hygrometers</a></li>
                            <li><a href="/search?categoryID=3">Liquids</a></li>
                            <li><a href="/search?categoryID=4">Lighters</a></li>
                            <li><a href="/search?categoryID=5">Cutters</a></li>
                            <li><a href="/search?categoryID=6">Ash Trays</a></li>
                        </ul>
                    </li>
                    <li class="menu_item has-children">
                        <a href="#">
                            My Account
                            <i class="fa fa-angle-down"></i>
                        </a>
                        <ul class="menu_selection">
                            <li><a href="/account"><i class="fa fa-sign-in" aria-hidden="true"></i>Sign In</a></li>
                            <li><a href="/register"><i class="fa fa-user-plus" aria-hidden="true"></i>Register</a></li>
                        </ul>
                    </li>
                    <li class="menu_item"><a href="/">home</a></li>
                    <li class="menu_item"><a href="/search">shop</a></li>
                    <li class="menu_item"><a href="/about">about</a></li>
                    <li class="menu_item"><a href="/contact">contact</a></li>
                </ul>
            </div>
        </div>

        @RenderBody()


        <!-- Footer -->

        <footer class="footer">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="footer_nav_container d-flex flex-sm-row flex-column align-items-center justify-content-lg-start justify-content-center text-center">
                            <ul class="footer_nav">
                                <li><a href="#">Blog</a></li>
                                <li><a href="#">FAQs</a></li>
                                <li><a href="contact.html">Contact us</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="footer_social d-flex flex-row align-items-center justify-content-lg-end justify-content-center">
                            <ul>
                                <li><a href="https://www.facebook.com/FamousSmokeShop"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                                <li><a href="https://www.twitter.com/famoussmokeshop"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                                <li><a href="https://www.instagram.com/famoussmokeshop"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                                <li><a href="https://www.youtube.com/user/FamousSmokeShopCigar"><i class="fa fa-youtube" aria-hidden="true"></i></a></li>
                                <li><a href="https://www.pinterest.com/FamousSmokeShop"><i class="fa fa-pinterest" aria-hidden="true"></i></a></li>
                                <li><a href="https://plus.google.com/u/0/+famoussmokeshop/posts"><i class="fa fa-google-plus" aria-hidden="true"></i></a></li>
                                <li><a href="https://www.yelp.com/biz/famous-smoke-shop-easton"><i class="fa fa-yelp" aria-hidden="true"></i></a></li>
                                <li><a href="https://www.linkedin.com/company/famous-smoke-shop/"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="footer_nav_container">
                            <div class="cr">©2018 All Rights Reserved. This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="#">Colorlib</a></div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

    </div>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @*@Scripts.Render("~/Scripts/theme/jquery-3.2.1.min.js")*@
    @Scripts.Render("~/Content/styles/bootstrap4/popper.js")
    @Scripts.Render("~/Content/styles/bootstrap4/bootstrap.min.js")
    @Scripts.Render("~/Scripts/plugins/Isotope/isotope.pkgd.min.js")
    @Scripts.Render("~/Scripts/plugins/OwlCarousel2-2.2.1/owl.carousel.js")
    @Scripts.Render("~/Scripts/plugins/easing/easing.js")
    @Scripts.Render("~/Scripts/theme/custom.js")
    @*@RenderSection("scripts", required: false)*@


</body>
</html>
